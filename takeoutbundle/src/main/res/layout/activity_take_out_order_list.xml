<?xml version="1.0" encoding="utf-8"?>
<com.yunos.tvtaobao.takeoutbundle.view.TOOrderListPositionManager
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/to_order_detail_list_root_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ol_gradient_shape_background"
    android:focusable="true" >

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal|top"
        android:layout_marginTop="@dimen/dp_50"
        android:shadowColor="@color/ytm_black"
        android:shadowDy="1"
        android:shadowRadius="1"
        android:text="外卖订单"
        android:textColor="@color/ytm_white"
        android:textSize="@dimen/sp_42" />

    <com.yunos.tvtaobao.takeoutbundle.view.TOInnerFocusHorizontalListView
        android:id="@+id/take_out_order_list_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingRight="@dimen/dp_60"
        android:paddingLeft="@dimen/dp_30"
        android:paddingTop="@dimen/dp_90"
        android:paddingBottom="@dimen/dp_60"
        android:layout_marginTop="@dimen/dp_100" />

</com.yunos.tvtaobao.takeoutbundle.view.TOOrderListPositionManager>