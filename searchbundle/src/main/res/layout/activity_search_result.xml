<?xml version="1.0" encoding="utf-8"?>
<com.yunos.tvtaobao.search.view.SearchResultLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.SearchResultActivity"
    android:background="@drawable/ytm_search_background">
    <FrameLayout
        android:id="@+id/fl_right_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="@dimen/dp_96"
        android:layout_marginLeft="@dimen/dp_224">

    </FrameLayout>



    <com.yunos.tvtaobao.search.view.TabView
        android:id="@+id/tv_tab_view"
        android:layout_width="@dimen/dp_196"
        android:layout_height="match_parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

</com.yunos.tvtaobao.search.view.SearchResultLayout>